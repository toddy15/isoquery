#: isoquery/cmdline_parser.py:30
msgid "Usage: %prog [options] [ISO codes]"
msgstr "Uso: %prog [opciones] [códigos ISO]"

#: isoquery/cmdline_parser.py:36
msgid "standard"
msgstr ""

